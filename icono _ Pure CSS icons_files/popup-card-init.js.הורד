(function(window){
try {
    if(window != window.top) return;
    
    var document = window.document;
    
    window.addEventListener("load", function(){
        // once load
        if(document.getElementById('netfree-popup-window-main')) return;
        
        var div = document.createElement('div');
        div.innerHTML = "<div id=\"netfree-popup-window\">\n    <style type=\"text/css\">\n            #netfree-popup-window-main {\n                position: fixed;\n                background: #FFFFFF;\n                padding: 0;\n                margin: 0;\n                transition: right 0.3s ease, left 0.3s ease;\n                z-index: 9999999999;\n                direction: rtl;\n                box-sizing: border-box;\n                position: fixed;\n                top: 0;\n                bottom: 0;\n                height: 100%;\n                right: -250px;\n                width: 250px;\n                \n            }\n            \n            #netfree-popup-window-hand-pull {\n                background-image: url(data:image/svg+xml;base64,);\n                background-size: 88%;\n                background-repeat: no-repeat;\n                width: 15px;\n                left: -15px;\n                height: 50px;\n                position: absolute;\n                top: 10px;\n                bottom: 10px;\n                margin: auto;\n                cursor: pointer;\n            }\n  \n            #netfree-popup-window-iframe {\n                width: 100%;\n                height: 100%;\n                border: 0;\n                margin: 0;\n                padding: 0;\n                position: relative;\n                display: block;\n            }\n            \n            .netfree-popup-window-mouseenter {\n                opacity: .7;\n                -webkit-filter: blur(2px)\n            }\n            \n            .netfree-popup-window-click {\n                opacity: .2\n            }\n            \n            \n            #netfree-popup-window-main.active {\n                right: 0px;\n            }\n            \n            #netfree-popup-window-main.h-position-right {\n                /* default */\n            }\n            \n            /* מוצמד לשמאל */\n            #netfree-popup-window-main.h-position-left {\n                right: auto;\n                left: -250px;\n            }\n            \n            #netfree-popup-window-main.h-position-left.active {\n                right: auto;\n                left: 0px;\n            }\n            \n            #netfree-popup-window-main.h-position-left #netfree-popup-window-hand-pull {\n                left: auto;\n                right: -15px;\n                background-position-x: 200%;\n            }\n            \n            /* נקודות למעלה */\n            #netfree-popup-window-main.v-position-top #netfree-popup-window-hand-pull {\n                margin: 20px 0 auto 0;\n            }\n            /* נקודות למטה */\n            #netfree-popup-window-main.v-position-bottom #netfree-popup-window-hand-pull {\n                margin: auto 0 20px 0;\n            }\n            \n            /* \n                מעלים את הכרטסת שמדפיסים את הדף\n            */\n            @media print {\n                #netfree-popup-window-main {\n                    display: none;\n                }\n                #netfree-popup-window{\n                    display: none;\n                }\n            }\n            \n            /*\n                מסך נמוך\n            */\n            @media only screen and (max-height: 750px) {\n                #netfree-popup-window-main {\n                    right: -309px;\n                    width: 309px;\n                }\n                #netfree-popup-window-main.h-position-left {\n                    left: -309px;\n                }\n            \n            }\n\n            /*\n                מסך שוכב של טאבלט\n            */\n            @media only screen and (max-device-width: 760px) and (orientation: landscape) {\n                #netfree-popup-window-main {\n                    right: -280px;\n                    width: 280px;\n                }\n                #netfree-popup-window-main.h-position-left {\n                    left: -280px;\n                }\n            }\n                \n            @media only screen and (max-device-width: 1200px) and (min-width: 760px) and (orientation: landscape) {\n                #netfree-popup-window-main {\n                    right: -302px;\n                    width: 302px;\n                }\n                #netfree-popup-window-main.h-position-left {\n                    left: -302px;\n                }\n            \n            }\n            \n            @media only screen and (max-device-width: 1200px) and (min-device-width: 750px) and (orientation: portrait) {\n                #netfree-popup-window-main {\n                    right: -270px;\n                    width: 270px;\n                }\n                #netfree-popup-window-main.h-position-left {\n                    left: -270px;\n                }\n            }\n            \n            @media only screen and (max-device-width: 700px) and (orientation: portrait) {\n                #netfree-popup-window-main {\n                    right: -222px;\n                    width: 222px;\n                }\n                #netfree-popup-window-main.h-position-left {\n                    left: -222px;\n                }\n            }\n            \n            @media print{\n                #netfree-popup-window-main{\n                    display: none;\n                }\n            }\n    </style>\n    \n    <div id=\"netfree-popup-window-main\">\n        <span id=\"netfree-popup-window-hand-pull\"></span>\n        <iframe id=\"netfree-popup-window-iframe\" src=\"https://netfree.link/card/card.html\"></iframe>\n    </div>\n    \n    <script type=\"text/javascript\" src=\"https://netfree.link/injection-script/popup-card.min.js\"></script>\n\n</div>\n";
        document.body.appendChild(div);
        
        var arr = div.getElementsByTagName('script');
        
        for (var n = 0; n < arr.length; n++){
            var newScript  = document.createElement('script');
            newScript.src  = arr[n].src;
            newScript.type = arr[n].type;
            newScript.text = arr[n].text;
            document.getElementsByTagName('head')[0].appendChild(newScript);
        }
    });
} catch (e) {}
})(window);